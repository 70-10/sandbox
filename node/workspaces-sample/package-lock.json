{
  "name": "workspaces-sample",
  "version": "0.1.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "0.1.0",
      "license": "MIT",
      "workspaces": [
        "apps/**"
      ]
    },
    "apps/module-1": {
      "version": "0.1.0",
